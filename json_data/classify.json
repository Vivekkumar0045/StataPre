  "data_collection_method": {
    "Quantitative": {
      "description": "Numerical data that can be measured and statistically analyzed. This forms the bulk of official statistics.",
      "data_variables": [
        "age",
        "income",
        "expenditure",
        "number_of_children",
        "literacy_rate",
        "unemployment_rate",
        "production_value",
        "enrolment_count",
        "test_scores"
      ]
    },
    "Qualitative": {
      "description": "Non-numerical, descriptive data that captures concepts, opinions, reasons, and experiences. Often collected through open-ended questions or in-depth interviews.",
      "data_variables": [
        "reasons_for_migration",
        "reasons_for_not_using_contraception",
        "perception_of_local_governance",
        "description_of_domestic_violence_incidents",
        "childs_experience_of_schooling",
        "reasons_for_dropping_out_of_school"
      ]
    },
    "Mixed Methods": {
      "description": "Surveys that integrate both quantitative and qualitative data collection to provide a comprehensive understanding.",
      "data_variables": [
        "combination_of_all_quantitative_variables_with_qualitative_reasoning_and_perceptions",
        "e.g., quantitative_data_on_vaccination_status_paired_with_qualitative_reasons_for_vaccine_hesitancy"
      ]
    }
  }
